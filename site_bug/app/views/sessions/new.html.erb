<% if flash[:danger] %>
  <div class="danger"> <%= flash[:danger] %>
<% end %>
<h1> login </h1>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>
